#string is the datatype
str= "python"
first_element_of_string= str[0]
print(first_element_of_string)
reverse_string= str[::-1]
print(reverse_string)
add_new_element= str + " is "+  "cool "
print(add_new_element)
#integer input
x= int(input())
y= int(input())
z= x+y;
print(z)
m= x-y;
print(m)
g= x%y;
print(g)
#list data type
thislist= ["a","b", "c"]
thislist.append("d")
print(thislist)
thislist.remove("a")
print(thislist)
thislist.clear()
print(thislist)
#tuple data type
thistuple =("apple", "banana", "cherry", "apple", "cherry")
print(thistuple)
#tuples are immutable
#set data type
thisset = {"apple", "banana", "cherry"}
print(thisset)
y= len(thisset)
print(y
